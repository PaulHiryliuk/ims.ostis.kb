sc_agent_check_identifiers
=> nrel_main_idtf:
	[sc-агент поиска всех sc-элементов, не имеющих основной идентификатор на некоторых естественных языках] 
	(* <- lang_ru;; *);
	[sc-agent of search of all sc-elements, not having main identifiers in some natural languages] 
	(* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_check_identifiers_condition => ..sc_agent_check_identifiers_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_check_identifiers
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_check_identifiers
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_check_identifiers;
		proc_finding_identifier
		};;
	-> sc_agent_check_identifiers_scp   
		(* <- active_sc_agent;;	*);;
	*);;

..sc_agent_check_identifiers_condition
= [*
	question_check_identifiers _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
*];;

..sc_agent_check_identifiers_result
= [*
	question_check_identifiers _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
*];;
